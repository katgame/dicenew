<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Main Page</title>
</head>
<body>
  <h1>Main Page</h1>
  <iframe id="shared-iframe" src="/iframe" width="600" height="400"></iframe>

  <script src="/socket.io/socket.io.js"></script>
  <script>
    const socket = io();

    // Listen for updates to the content
    socket.on('updateContent', (data) => {
      const iframe = document.getElementById('shared-iframe');
      // Potentially update iframe if content changes dynamically (Optional)
      // iframe.src = "/iframe";  // Re-load the iframe if necessary
    });
  </script>
</body>
</html>
